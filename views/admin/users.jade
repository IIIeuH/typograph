extends layout

block content
    .container-fluid
        .row
            .col-md-12
                table.table.table-striped.admin-table
                    tbody
                        each item in users
                            tr(onclick="window.location.href='users/"+item._id+"'; return false")
                                td=item.name
                                    i.material-icons.removeRow(id=item._id, data-name=item.name) delete_forever